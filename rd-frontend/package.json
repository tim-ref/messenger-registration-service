{
  "name": "frontend",
  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run gen && ng serve",
    "build": "npm run gen && ng build",
    "build:local": "npm run gen && ng build --configuration local",
    "build:kubernetes": "npm run gen && ng build --configuration kubernetes",
    "watch": "ng build --watch --configuration local",
    "test": "ng test",
    "test:headless": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "gen": "genversion tmp/version.js",
    "generate:messenger-instance-api-client": "openapi-generator-cli generate --generator-key messenger-instance-api",
    "generate:api-clients": "openapi-generator-cli generate",
    "generate:operator-instance-api-client": "openapi-generator-cli generate --generator-key operator-instance-api",
    "generate:logging-api-client": "openapi-generator-cli generate --generator-key logging-api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.4",
    "@angular/common": "^17.3.4",
    "@angular/compiler": "^17.3.4",
    "@angular/core": "^17.3.4",
    "@angular/forms": "^17.3.4",
    "@angular/platform-browser": "^17.3.4",
    "@angular/platform-browser-dynamic": "^17.3.4",
    "@angular/router": "^17.3.4",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "file-saver": "^2.0.5",
    "keycloak-angular": "^15.2.1",
    "keycloak-js": "^24.0.2",
    "ngx-cookie-service": "^17.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.3.3",
    "@openapitools/openapi-generator-cli": "^2.13.1",
    "@types/jasmine": "~4.6.4",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.12.7",
    "autoprefixer": "^10.4.19",
    "genversion": "^3.1.1",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.4"
  }
}
